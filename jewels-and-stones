class Solution {
    public int numJewelsInStones(String J, String S) {
        char[] j = J. toCharArray();
        char[] s = S. toCharArray();  
        int l=j.length;
        int m=s.length;
        int c=0;
        for(int i=0;i<l;i++)
        {
            for(int k=0;k<m;k++)
            {
                if(j[i]==s[k])
                    c++;
            }
        }
        return c;
    }
}
